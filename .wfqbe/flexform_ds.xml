<T3DataStructure>
 <meta>
       <langDisable>1</langDisable>
 </meta>
 <sheets>
  <sDEF>
   <ROOT>
    <TCEforms>
     <sheetTitle>LLL:EXT:wfqbe/locallang_db.xml:sheet_general</sheetTitle>
    </TCEforms>
    <type>array</type>
     <el>
	  <div_id>
    	<TCEforms>
			<label>LLL:EXT:wfqbe/locallang_db.xml:div_id</label>
    		<config>
	     		<type>input</type>
	     		<size>45</size>
	     		<max>45</max>
	     		<eval>trim</eval>
    		</config>
    	</TCEforms>
	</div_id>
      <queryObject>
		<TCEforms>
			<label>LLL:EXT:wfqbe/locallang_db.xml:query</label>
			<config>
				<type>group</type>
		        <internal_type>db</internal_type>
		        <allowed>tx_wfqbe_query</allowed>
		        <size>2</size>
		        <maxitems>1</maxitems>
		        <minitems>0</minitems>
		        <show_thumbs>1</show_thumbs>
			</config>
		</TCEforms>
	</queryObject>
	<parameterCheck>
       <TCEforms>
        <label>LLL:EXT:wfqbe/locallang_db.xml:parameterCheck</label>
        <config>
	     		<type>input</type>
	     		<size>45</size>
	     		<max>45</max>
	     		<eval>trim</eval>
        </config>
       </TCEforms>
      </parameterCheck>
	<templateFile>
		<TCEforms>
			<label>LLL:EXT:wfqbe/locallang_db.xml:templateFile</label>
			<config>
				<type>input</type>
				<eval>trim</eval>
				<default>EXT:wfqbe/pi1/wfqbe_template.html</default>
				<wizards type="array">
					<_PADDING>2</_PADDING>
					<link type="array">
						<type>popup</type>
						<title>Link</title>
						<icon>link_popup.gif</icon>
						<module type="array">
							<name>wizard_element_browser</name>
							<urlParameters type="array">
								<mode>wizard</mode>
								<act>file</act>
							</urlParameters>
						</module>
						<params type="array">
							<blindLinkOptions>page,url,mail,spec</blindLinkOptions>
						</params>
						<JSopenParams>height=300,width=500,status=0,menubar=0,scrollbars=1</JSopenParams>
					</link>
				</wizards>
			</config>
		</TCEforms>
	</templateFile>
			
	<customTemplate>
       <TCEforms>
        <label>LLL:EXT:wfqbe/locallang_db.xml:customTemplate</label>
        <config>
         <type>select</type>
         <items type="array">
		 <numIndex index="1" type="array">
           <numIndex index="0">LLL:EXT:wfqbe/locallang_db.xml:customTemplate.I.0</numIndex>
           <numIndex index="1"></numIndex>
           </numIndex>
          <numIndex index="2" type="array">
           <numIndex index="0">LLL:EXT:wfqbe/locallang_db.xml:customTemplate.I.1</numIndex>
           <numIndex index="1">0</numIndex>
           </numIndex>
          <numIndex index="3" type="array">
           <numIndex index="0">LLL:EXT:wfqbe/locallang_db.xml:customTemplate.I.2</numIndex>
           <numIndex index="1">1</numIndex>
          </numIndex>
          <numIndex index="4" type="array">
           <numIndex index="0">LLL:EXT:wfqbe/locallang_db.xml:customTemplate.I.3</numIndex>
           <numIndex index="1">2</numIndex>
          </numIndex>
         </items>
         <minitems>0</minitems>
         <maxitems>1</maxitems>
         <size>1</size>
        </config>
       </TCEforms>
      </customTemplate>
	<debugQuery>
       <TCEforms>
        <label>LLL:EXT:wfqbe/locallang_db.xml:debugQuery</label>
        <config>
         <type>check</type>
        </config>
       </TCEforms>
      </debugQuery>
     </el>
    </ROOT>
   </sDEF>
   
   
   <results>
    <ROOT>
     <TCEforms>
      <sheetTitle>LLL:EXT:wfqbe/locallang_db.xml:sheet_results</sheetTitle>
     </TCEforms>
     <type>array</type>
     <el>
      <recordsForPage>
    	<TCEforms>
			<label>LLL:EXT:wfqbe/locallang_db.xml:max_items_in_list</label>
    		<config>
	     		<type>input</type>
	     		<size>5</size>
	     		<max>5</max>
	     		<eval>required,trim,num,int</eval>
	     		<range>
	     			<lower>1</lower>
	     		</range>
	     		<default>5</default>
    		</config>
    	</TCEforms>
	</recordsForPage>
	<emptyResult>
    	<TCEforms>
			<label>LLL:EXT:wfqbe/locallang_db.xml:emptyResult</label>
    		<config>
	     		<type>text</type>
	     		<size>5</size>
	     		<max>3</max>
    		</config>
    	</TCEforms>
	</emptyResult>
	<summary>
    	<TCEforms>
			<label>LLL:EXT:wfqbe/locallang_db.xml:summary</label>
    		<config>
	     		<type>input</type>
	     		<size>45</size>
	     		<max>255</max>
	     		<eval>trim</eval>
    		</config>
    	</TCEforms>
	</summary>
	<caption>
    	<TCEforms>
			<label>LLL:EXT:wfqbe/locallang_db.xml:caption</label>
    		<config>
	     		<type>input</type>
	     		<size>45</size>
	     		<max>255</max>
	     		<eval>trim</eval>
    		</config>
    	</TCEforms>
	</caption>
	<csvDownload>
    	<TCEforms>
			<label>LLL:EXT:wfqbe/locallang_db.xml:csvDownload</label>
    		<config>
	         <type>check</type>
	        </config>
    	</TCEforms>
	  </csvDownload>
     </el>
    </ROOT>
   </results>
   
   
   <search>
    <ROOT>
     <TCEforms>
      <sheetTitle>LLL:EXT:wfqbe/locallang_db.xml:sheet_search</sheetTitle>
     </TCEforms>
     <type>array</type>
     <el>
      <resultsPage>
    	<TCEforms>
			<label>LLL:EXT:wfqbe/locallang_db.xml:resultsPage</label>
    		<config>
				<type>group</type>
		        <internal_type>db</internal_type>
		        <allowed>pages</allowed>
		        <size>2</size>
		        <maxitems>1</maxitems>
		        <minitems>0</minitems>
		        <show_thumbs>1</show_thumbs>
			</config>
    	</TCEforms>
	</resultsPage>
	<div_search_result_id>
    	<TCEforms>
			<label>LLL:EXT:wfqbe/locallang_db.xml:div_search_result_id</label>
    		<config>
	     		<type>input</type>
	     		<size>45</size>
	     		<max>45</max>
	     		<eval>trim</eval>
    		</config>
    	</TCEforms>
	</div_search_result_id>
     </el>
    </ROOT>
   </search>
   
   
   <insert>
    <ROOT>
     <TCEforms>
      <sheetTitle>LLL:EXT:wfqbe/locallang_db.xml:sheet_insert</sheetTitle>
     </TCEforms>
     <type>array</type>
     <el>
      <pageConfirmation>
    	<TCEforms>
			<label>LLL:EXT:wfqbe/locallang_db.xml:pageConfirmation</label>
    		<config>
	         <type>check</type>
	        </config>
    	</TCEforms>
	  </pageConfirmation>
	  <detailedPage>
    	<TCEforms>
			<label>LLL:EXT:wfqbe/locallang_db.xml:detailedPage</label>
    		<config>
				<type>group</type>
		        <internal_type>db</internal_type>
		        <allowed>pages</allowed>
		        <size>2</size>
		        <maxitems>1</maxitems>
		        <minitems>0</minitems>
		        <show_thumbs>1</show_thumbs>
			</config>
    	</TCEforms>
	  </detailedPage>
	  <redirectQuery>
    	<TCEforms>
			<label>LLL:EXT:wfqbe/locallang_db.xml:redirectQuery</label>
    		<config>
	     		<type>input</type>
	     		<size>80</size>
	     		<max>255</max>
	     		<eval>trim</eval>
    		</config>
    	</TCEforms>
	  </redirectQuery>
	  <deletedPage>
    	<TCEforms>
			<label>LLL:EXT:wfqbe/locallang_db.xml:deletedPage</label>
    		<config>
				<type>group</type>
		        <internal_type>db</internal_type>
		        <allowed>pages</allowed>
		        <size>2</size>
		        <maxitems>1</maxitems>
		        <minitems>0</minitems>
		        <show_thumbs>1</show_thumbs>
			</config>
    	</TCEforms>
	  </deletedPage>
	  <clear_cache>
    	<TCEforms>
			<label>LLL:EXT:wfqbe/locallang_db.xml:clear_cache</label>
    		<config>
	     		<type>input</type>
	     		<size>45</size>
	     		<max>255</max>
	     		<eval>trim</eval>
    		</config>
    	</TCEforms>
	   </clear_cache>
	   <send_email>
    	<TCEforms>
			<label>LLL:EXT:wfqbe/locallang_db.xml:send_email</label>
    		<config>
	     		<type>check</type>
    		</config>
    	</TCEforms>
	  </send_email>
	  <notify_email>
    	<TCEforms>
			<label>LLL:EXT:wfqbe/locallang_db.xml:notify_email</label>
    		<config>
	     		<type>input</type>
	     		<size>45</size>
	     		<max>255</max>
	     		<eval>trim</eval>
    		</config>
    	</TCEforms>
	   </notify_email>
	   <notify_subject>
    	<TCEforms>
			<label>LLL:EXT:wfqbe/locallang_db.xml:notify_subject</label>
    		<config>
	     		<type>input</type>
	     		<size>45</size>
	     		<max>255</max>
	     		<eval>trim</eval>
    		</config>
    	</TCEforms>
	   </notify_subject>
	   <send_email_user>
    	<TCEforms>
			<label>LLL:EXT:wfqbe/locallang_db.xml:send_email_user</label>
    		<config>
	     		<type>check</type>
    		</config>
    	</TCEforms>
	  </send_email_user>
	  <field_email_user>
    	<TCEforms>
			<label>LLL:EXT:wfqbe/locallang_db.xml:field_email_user</label>
    		<config>
	     		<type>input</type>
	     		<size>45</size>
	     		<max>255</max>
	     		<eval>trim</eval>
    		</config>
    	</TCEforms>
	   </field_email_user>
	   <notify_subject_user>
    	<TCEforms>
			<label>LLL:EXT:wfqbe/locallang_db.xml:notify_subject_user</label>
    		<config>
	     		<type>input</type>
	     		<size>45</size>
	     		<max>255</max>
	     		<eval>trim</eval>
    		</config>
    	</TCEforms>
	   </notify_subject_user>
	   <mailTemplate>
		<TCEforms>
			<label>LLL:EXT:wfqbe/locallang_db.xml:mailTemplate</label>
			<config>
				<type>input</type>
				<eval>trim</eval>
				<default></default>
				<wizards type="array">
					<_PADDING>2</_PADDING>
					<link type="array">
						<type>popup</type>
						<title>Link</title>
						<icon>link_popup.gif</icon>
						<module type="array">
							<name>wizard_element_browser</name>
							<urlParameters type="array">
								<mode>wizard</mode>
								<act>file</act>
							</urlParameters>
						</module>
						<params type="array">
							<blindLinkOptions>page,url,mail,spec</blindLinkOptions>
						</params>
						<JSopenParams>height=300,width=500,status=0,menubar=0,scrollbars=1</JSopenParams>
					</link>
				</wizards>
			</config>
		</TCEforms>
	</mailTemplate>
     </el>
    </ROOT>
   </insert>
   
      
  </sheets>
</T3DataStructure>

