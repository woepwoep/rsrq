<?xml version="1.0" encoding="UTF-8"?>
<T3DataStructure>
    <ROOT>
        <TCEforms>
            <sheetTitle>LLL:EXT:rsrq/Resources/Private/Language/Flexform_cud.xlf:sheet_database</sheetTitle>
        </TCEforms>
        <type>array</type>
        <el>
            <targetTable>
                <label>LLL:EXT:rsrq/Resources/Private/Language/Flexform_cud.xlf:targetTable</label>
                <onChange>reload</onChange>
                <config>
                    <type>select</type>
                    <renderType>selectSingle</renderType>
                    <itemsProcFunc>RedSeadog\Rsrq\UserFunc\Field->populateTargetTable</itemsProcFunc>
                    <minitems>0</minitems>
                    <maxitems>1</maxitems>
                    <size>1</size>
                </config>
            </targetTable>
            <identifiers>
                <label>LLL:EXT:rsrq/Resources/Private/Language/Flexform_cud.xlf:identifiers</label>
                <config>
                    <type>input</type>
                    <size>45</size>
                    <max>45</max>
                    <eval>trim,required</eval>
                </config>
            </identifiers>
            <fieldlist>
                <label>LLL:EXT:rsrq/Resources/Private/Language/Flexform_cud.xlf:fieldlist</label>
                <onChange>reload</onChange>
                <config>
                    <type>select</type>
                    <renderType>selectMultipleSideBySide</renderType>
                    <enableMultiSelectFilterTextfield>true</enableMultiSelectFilterTextfield>
                    <itemsProcFunc>RedSeadog\Rsrq\UserFunc\Field->getColumnNames</itemsProcFunc>
                    <minitems>0</minitems>
                    <maxitems>45</maxitems>
                    <size>5</size>
                </config>
            </fieldlist>
            <fieldtypes>
                <section>1</section>
                <type>array</type>
                <title>LLL:EXT:rsrq/Resources/Private/Language/Flexform_cud.xlf:fieldtypes</title>
                <el>
                    <typesection>
                        <type>array</type>
                        <el>
                            <nodisplay>
                                <label>noDisplay</label>
                                <config>
                                    <type>check</type>
                                    <items type="array">
                                        <numIndex index="0" type="array">
                                            <numIndex index="0"></numIndex>
                                            <numIndex index="1">noDisplay</numIndex>
                                        </numIndex>
                                    </items>
                                </config>
                            </nodisplay>
                            <veldtype>
                                <label>LLL:EXT:rsrq/Resources/Private/Language/Flexform_cud.xlf:veldtype</label>
                                <onChange>reload</onChange>
                                <config>
                                    <type>select</type>
                                    <renderType>selectSingle</renderType>
                                    <itemsProcFunc>RedSeadog\Rsrq\UserFunc\Field->populateInputFieldTypes</itemsProcFunc>
                                    <autosizemax>2</autosizemax>
                                    <size>25</size>
                                    <maxitems>1</maxitems>
                                    <minitems>1</minitems>
                                    <size>1</size>
                                </config>
                            </veldtype>
                            <argument>
                                <label>LLL:EXT:rsrq/Resources/Private/Language/Flexform_cud.xlf:argument</label>
                                <config>
                                    <type>text</type>
                                    <cols>45</cols>
                                    <rows>5</rows>
                                    <eval>trim</eval>
                                </config>
                                <displayCond>FIELD:veldtype:=:select</displayCond>
                            </argument>
                            <optionlist>
                                <label>LLL:EXT:rsrq/Resources/Private/Language/Flexform_cud.xlf:optionlist</label>
                                <config>
                                    <type>text</type>
                                    <cols>45</cols>
                                    <rows>5</rows>
                                    <eval>trim</eval>
                                </config>
                                <displayCond>
                                    <or>
                                        <value2>FIELD:veldtype:=:radio</value2>
                                        <value3>FIELD:veldtype:=:selectarray</value3>
                                    </or>
                                </displayCond>
                            </optionlist>
                            <name>
                                <TCEforms>
                                    <label>LLL:EXT:rsrq/Resources/Private/Language/Flexform_cud.xlf:name</label>
                                    <config>
                                        <type>select</type>
                                        <renderType>selectSingle</renderType>
                                        <itemsProcFunc>RedSeadog\Rsrq\UserFunc\Field->populateFieldNames</itemsProcFunc>
                                        <minitems>0</minitems>
                                        <maxitems>1</maxitems>
                                        <size>1</size>
                                    </config>
                                </TCEforms>
                            </name>
                            <additionalargument>
                                <label>LLL:EXT:rsrq/Resources/Private/Language/Flexform_cud.xlf:additionalargument</label>
                                <config>
                                    <type>input</type>
                                    <cols>15</cols>
                                    <eval>trim</eval>
                                </config>
                                <displayCond>FIELD:veldtype:!=:nodisplay</displayCond>
                            </additionalargument>
                        </el>
                    </typesection>
                </el>
            </fieldtypes>
            <defaultvalues>
                <label>LLL:EXT:rsrq/Resources/Private/Language/Flexform_cud.xlf:fixedvalues</label>
                <config>
                    <type>text</type>
                    <cols>45</cols>
                    <rows>5</rows>
                    <eval>trim</eval>
                </config>
            </defaultvalues>
        </el>
    </ROOT>
</T3DataStructure>
