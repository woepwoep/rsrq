<?xml version="1.0" encoding="UTF-8"?>
<T3DataStructure>
    <ROOT>
        <TCEforms>
            <sheetTitle>LLL:EXT:wfqbe/Resources/Private/Language/Flexform_query.xlf:sheet_results</sheetTitle>
            <sheetDescription></sheetDescription>
            <sheetShortDescr></sheetShortDescr>
        </TCEforms>
        <type>array</type>
        <el>
            <recordsForPage>
                <TCEforms>
                    <label>LLL:EXT:wfqbe/Resources/Private/Language/Flexform_query.xlf:recordsForPage</label>
                    <config>
                        <type>input</type>
                        <size>5</size>
                        <max>3</max>
                        <eval>required,trim,num,int</eval>
                        <range>
                            <lower>1</lower>
                        </range>
                        <default>25</default>
                    </config>
                </TCEforms>
            </recordsForPage>
            <slidingPages>
                <TCEforms>
                    <label>LLL:EXT:wfqbe/Resources/Private/Language/Flexform_query.xlf:slidingPages</label>
                    <config>
                        <type>input</type>
                        <size>5</size>
                        <max>3</max>
                        <eval>trim,num,int</eval>
                        <default>20</default>
                    </config>
                </TCEforms>
            </slidingPages>
            <emptyResult>
                <TCEforms>
                    <label>LLL:EXT:wfqbe/Resources/Private/Language/Flexform_query.xlf:emptyResult</label>
                    <config>
                        <type>text</type>
                        <size>50</size>
                        <enableRichtext>0</enableRichtext>
                    </config>
                </TCEforms>
            </emptyResult>
            <csvDownload>
                <TCEforms>
                    <label>LLL:EXT:wfqbe/Resources/Private/Language/Flexform_query.xlf:csvDownload</label>
                    <config>
                        <type>check</type>
                    </config>
                </TCEforms>
            </csvDownload>
        </el>
    </ROOT>
</T3DataStructure>
