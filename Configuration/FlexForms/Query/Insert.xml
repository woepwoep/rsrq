<T3DataStructure>
    <ROOT>
        <TCEforms>
            <sheetTitle>LLL:EXT:rsrq/Resources/Private/Language/FfQuery.xlf:sheet_insert</sheetTitle>
        </TCEforms>
        <type>array</type>
        <el>
            <pageConfirmation>
                <TCEforms>
                    <label>LLL:EXT:rsrq/Resources/Private/Language/FfQuery.xlf:pageConfirmation</label>
                    <config>
                        <type>check</type>
                    </config>
                </TCEforms>
            </pageConfirmation>
            <detailedPage>
                <TCEforms>
                    <label>LLL:EXT:rsrq/Resources/Private/Language/FfQuery.xlf:detailedPage</label>
                    <config>
                        <type>group</type>
                        <internal_type>db</internal_type>
                        <allowed>pages</allowed>
                        <size>2</size>
                        <maxitems>1</maxitems>
                        <minitems>0</minitems>
                        <show_thumbs>1</show_thumbs>
                    </config>
                </TCEforms>
            </detailedPage>
            <redirectQuery>
                <TCEforms>
                    <label>LLL:EXT:rsrq/Resources/Private/Language/FfQuery.xlf:redirectQuery</label>
                    <config>
                        <type>input</type>
                        <size>80</size>
                        <max>255</max>
                        <eval>trim</eval>
                    </config>
                </TCEforms>
            </redirectQuery>
            <deletedPage>
                <TCEforms>
                    <label>LLL:EXT:rsrq/Resources/Private/Language/FfQuery.xlf:deletedPage</label>
                    <config>
                        <type>group</type>
                        <internal_type>db</internal_type>
                        <allowed>pages</allowed>
                        <size>2</size>
                        <maxitems>1</maxitems>
                        <minitems>0</minitems>
                        <show_thumbs>1</show_thumbs>
                    </config>
                </TCEforms>
            </deletedPage>
            <clear_cache>
                <TCEforms>
                    <label>LLL:EXT:rsrq/Resources/Private/Language/FfQuery.xlf:clear_cache</label>
                    <config>
                        <type>input</type>
                        <size>45</size>
                        <max>255</max>
                        <eval>trim</eval>
                    </config>
                </TCEforms>
            </clear_cache>
            <send_email>
                <TCEforms>
                    <label>LLL:EXT:rsrq/Resources/Private/Language/FfQuery.xlf:send_email</label>
                    <config>
                        <type>check</type>
                    </config>
                </TCEforms>
            </send_email>
            <notify_email>
                <TCEforms>
                    <label>LLL:EXT:rsrq/Resources/Private/Language/FfQuery.xlf:notify_email</label>
                    <config>
                        <type>input</type>
                        <size>45</size>
                        <max>255</max>
                        <eval>trim</eval>
                    </config>
                </TCEforms>
            </notify_email>
            <notify_subject>
                <TCEforms>
                    <label>LLL:EXT:rsrq/Resources/Private/Language/FfQuery.xlf:notify_subject</label>
                    <config>
                        <type>input</type>
                        <size>45</size>
                        <max>255</max>
                        <eval>trim</eval>
                    </config>
                </TCEforms>
            </notify_subject>
            <send_email_user>
                <TCEforms>
                    <label>LLL:EXT:rsrq/Resources/Private/Language/FfQuery.xlf:send_email_user</label>
                    <config>
                        <type>check</type>
                    </config>
                </TCEforms>
            </send_email_user>
            <field_email_user>
                <TCEforms>
                    <label>LLL:EXT:rsrq/Resources/Private/Language/FfQuery.xlf:field_email_user</label>
                    <config>
                        <type>input</type>
                        <size>45</size>
                        <max>255</max>
                        <eval>trim</eval>
                    </config>
                </TCEforms>
            </field_email_user>
            <notify_subject_user>
                <TCEforms>
                    <label>LLL:EXT:rsrq/Resources/Private/Language/FfQuery.xlf:notify_subject_user</label>
                    <config>
                        <type>input</type>
                        <size>45</size>
                        <max>255</max>
                        <eval>trim</eval>
                    </config>
                </TCEforms>
            </notify_subject_user>
            <mailTemplate>
                <TCEforms>
                    <label>LLL:EXT:rsrq/Resources/Private/Language/FfQuery.xlf:mailTemplate</label>
                    <config>
                        <type>input</type>
                        <eval>trim</eval>
                        <default></default>
                        <wizards type="array">
                            <_PADDING>2</_PADDING>
                            <link type="array">
                                <type>popup</type>
                                <title>Link</title>
                                <icon>link_popup.gif</icon>
                                <module type="array">
                                   <name>wizard_element_browser</name>
                                   <urlParameters type="array">
                                       <mode>wizard</mode>
                                       <act>file</act>
                                   </urlParameters>
                                </module>
                                <params type="array">
                                   <blindLinkOptions>page,url,mail,spec</blindLinkOptions>
                                </params>
                                <JSopenParams>height=300,width=500,status=0,menubar=0,scrollbars=1</JSopenParams>
                            </link>
                        </wizards>
                    </config>
                </TCEforms>
            </mailTemplate>
        </el>
    </ROOT>
</T3DataStructure>
