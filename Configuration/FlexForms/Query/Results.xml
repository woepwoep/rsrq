<T3DataStructure>
    <ROOT>
        <TCEforms>
            <sheetTitle>LLL:EXT:rsrq/Resources/Private/Language/FfQuery.xlf:sheet_results</sheetTitle>
        </TCEforms>
        <type>array</type>
        <el>
            <recordsForPage>
                <TCEforms>
                    <label>LLL:EXT:rsrq/Resources/Private/Language/FfQuery.xlf:max_items_in_list</label>
                    <config>
                        <type>input</type>
                        <size>5</size>
                        <max>5</max>
                        <eval>required,trim,num,int</eval>
                        <range>
                            <lower>1</lower>
                        </range>
                        <default>5</default>
                    </config>
                </TCEforms>
            </recordsForPage>
            <emptyResult>
                <TCEforms>
                    <label>LLL:EXT:rsrq/Resources/Private/Language/FfQuery.xlf:emptyResult</label>
                    <config>
                        <type>text</type>
                        <size>5</size>
                        <max>3</max>
                    </config>
                </TCEforms>
            </emptyResult>
            <summary>
                <TCEforms>
                    <label>LLL:EXT:rsrq/Resources/Private/Language/FfQuery.xlf:summary</label>
                    <config>
                        <type>input</type>
                        <size>45</size>
                        <max>255</max>
                        <eval>trim</eval>
                    </config>
                </TCEforms>
            </summary>
            <caption>
                <TCEforms>
                    <label>LLL:EXT:rsrq/Resources/Private/Language/FfQuery.xlf:caption</label>
                    <config>
                        <type>input</type>
                        <size>45</size>
                        <max>255</max>
                        <eval>trim</eval>
                    </config>
                </TCEforms>
            </caption>
            <csvDownload>
                <TCEforms>
                    <label>LLL:EXT:rsrq/Resources/Private/Language/FfQuery.xlf:csvDownload</label>
                    <config>
                        <type>check</type>
                    </config>
                </TCEforms>
            </csvDownload>
        </el>
    </ROOT>
</T3DataStructure>
