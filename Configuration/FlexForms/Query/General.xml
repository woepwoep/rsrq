<T3DataStructure>
    <ROOT>
        <TCEforms>
            <sheetTitle>LLL:EXT:rsrq/Resources/Private/Language/FfQuery.xlf:sheet_general</sheetTitle>
        </TCEforms>
        <type>array</type>
        <el>
            <div_id>
                <TCEforms>
                    <label>LLL:EXT:rsrq/Resources/Private/Language/FfQuery.xlf:div_id</label>
                    <config>
                        <type>input</type>
                        <size>45</size>
                        <max>45</max>
                        <eval>trim</eval>
                    </config>
                </TCEforms>
            </div_id>
            <queryObject>
                <TCEforms>
                    <label>LLL:EXT:rsrq/Resources/Private/Language/FfQuery.xlf:query</label>
                    <config>
                        <type>group</type>
                        <internal_type>db</internal_type>
                        <allowed>tx_rsrq_domain_model_query</allowed>
                        <size>2</size>
                        <maxitems>1</maxitems>
                        <minitems>0</minitems>
                        <show_thumbs>1</show_thumbs>
                    </config>
                </TCEforms>
            </queryObject>
            <parameterCheck>
                <TCEforms>
                    <label>LLL:EXT:rsrq/Resources/Private/Language/FfQuery.xlf:parameterCheck</label>
                    <config>
                        <type>input</type>
                        <size>45</size>
                        <max>45</max>
                        <eval>trim</eval>
                    </config>
                </TCEforms>
            </parameterCheck>
            <templateFile>
                <TCEforms>
                    <label>LLL:EXT:rsrq/Resources/Private/Language/FfQuery.xlf:templateFile</label>
                    <config>
                        <type>input</type>
                        <eval>trim</eval>
                        <default>EXT:rsrq/pi1/rsrq_template.html</default>
                        <wizards type="array">
                            <_PADDING>2</_PADDING>
                            <link type="array">
                                <type>popup</type>
                                <title>Link</title>
                                <icon>link_popup.gif</icon>
                                <module type="array">
                                    <name>wizard_element_browser</name>
                                    <urlParameters type="array">
                                        <mode>wizard</mode>
                                        <act>file</act>
                                    </urlParameters>
                                </module>
                                <params type="array">
                                    <blindLinkOptions>page,url,mail,spec</blindLinkOptions>
                                </params>
                                <JSopenParams>height=300,width=500,status=0,menubar=0,scrollbars=1</JSopenParams>
                            </link>
                        </wizards>
                    </config>
                </TCEforms>
            </templateFile>
            <customTemplate>
                <TCEforms>
                    <label>LLL:EXT:rsrq/Resources/Private/Language/FfQuery.xlf:customTemplate</label>
                    <config>
                        <type>select</type>
                        <items type="array">
                            <numIndex index="1" type="array">
                                <numIndex index="0">LLL:EXT:rsrq/Resources/Private/Language/FfQuery.xlf:customTemplate.I.0</numIndex>
                                <numIndex index="1"></numIndex>
                            </numIndex>
                            <numIndex index="2" type="array">
                                <numIndex index="0">LLL:EXT:rsrq/Resources/Private/Language/FfQuery.xlf:customTemplate.I.1</numIndex>
                                <numIndex index="1">0</numIndex>
                            </numIndex>
                            <numIndex index="3" type="array">
                                <numIndex index="0">LLL:EXT:rsrq/Resources/Private/Language/FfQuery.xlf:customTemplate.I.2</numIndex>
                                <numIndex index="1">1</numIndex>
                            </numIndex>
                            <numIndex index="4" type="array">
                                <numIndex index="0">LLL:EXT:rsrq/Resources/Private/Language/FfQuery.xlf:customTemplate.I.3</numIndex>
                                <numIndex index="1">2</numIndex>
                            </numIndex>
                        </items>
                        <minitems>0</minitems>
                        <maxitems>1</maxitems>
                        <size>1</size>
                    </config>
                </TCEforms>
            </customTemplate>
            <debugQuery>
                <TCEforms>
                    <label>LLL:EXT:rsrq/Resources/Private/Language/FfQuery.xlf:debugQuery</label>
                    <config>
                        <type>check</type>
                    </config>
                </TCEforms>
            </debugQuery>
        </el>
    </ROOT>
</T3DataStructure>
